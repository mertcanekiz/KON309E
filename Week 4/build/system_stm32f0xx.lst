 144:Project/Demonstration/system_stm32f0xx.c **** /**
 145:Project/Demonstration/system_stm32f0xx.c ****   * @brief  Setup the microcontroller system.
 146:Project/Demonstration/system_stm32f0xx.c ****   *         Initialize the Embedded Flash Interface, the PLL and update the 
 147:Project/Demonstration/system_stm32f0xx.c ****   *         SystemCoreClock variable.
ARM GAS  /tmp/ccCNnXNc.s 			page 4


 148:Project/Demonstration/system_stm32f0xx.c ****   * @param  None
 149:Project/Demonstration/system_stm32f0xx.c ****   * @retval None
 150:Project/Demonstration/system_stm32f0xx.c ****   */
 151:Project/Demonstration/system_stm32f0xx.c **** void SystemInit (void)
 152:Project/Demonstration/system_stm32f0xx.c **** {    
 153:Project/Demonstration/system_stm32f0xx.c ****   /* Set HSION bit */
 154:Project/Demonstration/system_stm32f0xx.c ****   RCC->CR |= (uint32_t)0x00000001;
 155:Project/Demonstration/system_stm32f0xx.c **** 
 156:Project/Demonstration/system_stm32f0xx.c ****   /* Reset SW[1:0], HPRE[3:0], PPRE[2:0], ADCPRE and MCOSEL[2:0] bits */
 157:Project/Demonstration/system_stm32f0xx.c ****   RCC->CFGR &= (uint32_t)0xF8FFB80C;
 158:Project/Demonstration/system_stm32f0xx.c ****   
 159:Project/Demonstration/system_stm32f0xx.c ****   /* Reset HSEON, CSSON and PLLON bits */
 160:Project/Demonstration/system_stm32f0xx.c ****   RCC->CR &= (uint32_t)0xFEF6FFFF;
 161:Project/Demonstration/system_stm32f0xx.c **** 
 162:Project/Demonstration/system_stm32f0xx.c ****   /* Reset HSEBYP bit */
 163:Project/Demonstration/system_stm32f0xx.c ****   RCC->CR &= (uint32_t)0xFFFBFFFF;
 164:Project/Demonstration/system_stm32f0xx.c **** 
 165:Project/Demonstration/system_stm32f0xx.c ****   /* Reset PLLSRC, PLLXTPRE and PLLMUL[3:0] bits */
 166:Project/Demonstration/system_stm32f0xx.c ****   RCC->CFGR &= (uint32_t)0xFFC0FFFF;
 167:Project/Demonstration/system_stm32f0xx.c **** 
 168:Project/Demonstration/system_stm32f0xx.c ****   /* Reset PREDIV1[3:0] bits */
 169:Project/Demonstration/system_stm32f0xx.c ****   RCC->CFGR2 &= (uint32_t)0xFFFFFFF0;
 170:Project/Demonstration/system_stm32f0xx.c **** 
 171:Project/Demonstration/system_stm32f0xx.c ****   /* Reset USARTSW[1:0], I2CSW, CECSW and ADCSW bits */
 172:Project/Demonstration/system_stm32f0xx.c ****   RCC->CFGR3 &= (uint32_t)0xFFFFFEAC;
 173:Project/Demonstration/system_stm32f0xx.c **** 
 174:Project/Demonstration/system_stm32f0xx.c ****   /* Reset HSI14 bit */
 175:Project/Demonstration/system_stm32f0xx.c ****   RCC->CR2 &= (uint32_t)0xFFFFFFFE;
 176:Project/Demonstration/system_stm32f0xx.c **** 
 177:Project/Demonstration/system_stm32f0xx.c ****   /* Disable all interrupts */
 178:Project/Demonstration/system_stm32f0xx.c ****   RCC->CIR = 0x00000000;
 179:Project/Demonstration/system_stm32f0xx.c **** 
 180:Project/Demonstration/system_stm32f0xx.c ****   /* Configure the System clock frequency, AHB/APBx prescalers and Flash settings */
 181:Project/Demonstration/system_stm32f0xx.c ****   SetSysClock();
 182:Project/Demonstration/system_stm32f0xx.c **** }
